{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "Recreational Fishing in Australia by State",
  "width": 800,
  "height": 600,
  "projection": {
    "type": "mercator"
  },
  "layer": [
    {
      "data": {
        "url": "https://github.com/GanyuGuo/FIT3179-Data-Visualisation-2-Recreational-fishing-in-Australia/blob/main/js/Australia.topojson",
        "format": {
          "type": "topojson"
          "feature": "ne_10m_admin_1_states_provinces"
        }
      },
      "mark": {
        "type": "geoshape",
        "stroke": "white",
        "strokeWidth": 1
      },
      "encoding": {
        "color": {
          "field": "properties.percentage",
          "type": "quantitative",
          "title": "Percentage of Population"
        }
      }
    }
  ]
}
